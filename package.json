    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"libs/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
        "migration:generate": "yarn run typeorm migration:generate src/database/migrations/migration -d src/database/data-source.config.ts",
        "migration:run": "yarn run typeorm migration:run -d src/database/data-source.config.ts",
        "migration:revert": "yarn run typeorm migration:revert -d src/database/data-source.config.ts",
        "seed:run": "yarn run typeorm migration:run -d src/database/data-source.config.ts",
        "database:init": "yarn migration:generate && yarn migration:run && yarn seed:run"
    },
    "packageManager": "yarn@4.2.2",
